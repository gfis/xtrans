<record name="URIScheme" package="org.teherba.xtrans.net" 
		type="Bean" remark="URI Schemes as defined by IANA" length="51" sql="false" >
    <field name="scheme"    rem="scheme"                    type="char" />
    <field name="schemeSpecificPart"    rem="scheme specific part"  type="char" />
    <field name="authority" rem="authority"                 type="char" />
    <field name="userInfo"  rem="userInfo"                  type="char" />
    <field name="host"      rem="DNS name or IP address"    type="char" />
    <field name="port"      rem="port number"               type="number" />
    <field name="path"      rem="path and filename"         type="char" />
    <field name="query"     rem="key=value pairs for parameters"    type="char" />
    <field name="fragment"  rem="specific fragment in the file"     type="char" />
    <field name="params"    rem="parameter map"             type="char" />
</record>
