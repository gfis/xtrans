<record xmlns="http://www.teherba.org/2006/xtrans/Record"
		name="LinkOEIS" package="org.teherba.xtrans.misc" type="Bean" order="LSB" code="ASCII" length="585" 
        remark="Table columns for URL mapping" sql="true" >
    <field name="seqNo" 	rem="sequence number Annnnnnn"    	len="8"     type="String"   />
    <field name="oldPath" 	rem="old URL without filename"    	len="180"   type="String"   />
    <field name="oldFile" 	rem="old filename from URL"       	len="128"   type="String"   />
    <field name="oldRevDom" rem="old reversed domain"    		len="100"   type="String"   />
    <field name="status"    rem="HTTP request return code"		len="6"		type="String"   />
    <field name="title" 	rem="content of HTML {a} element"  	len="256"   type="String"   />
    <field name="newPath" 	rem="new URL without filename"    	len="256"   type="String"   />
    <field name="newFile" 	rem="new filename from URL"       	len="256"   type="String"   />
    <field name="comment" 	rem="comment for the correction"  	len="256"   type="String"   />
</record>
